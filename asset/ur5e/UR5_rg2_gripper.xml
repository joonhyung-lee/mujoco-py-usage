        <exclude body1="base_link" body2="shoulder_link"/>
        <exclude body1="shoulder_link" body2="upper_arm_link"/>
        <exclude body1="upper_arm_link" body2="forearm_link"/>
        <exclude body1="forearm_link" body2="wrist_1_link"/>
        <exclude body1="wrist_1_link" body2="wrist_2_link"/>
        <exclude body1="wrist_2_link" body2="wrist_3_link"/>
        <exclude body1="rg2_gripper_base_link" body2="wrist_3_link"/>

        <exclude body1="rg2_gripper_base_link" body2="rg2_gripper_finger1_finger_link"/>
        <exclude body1="rg2_gripper_base_link" body2="rg2_gripper_finger2_finger_link"/>
        <exclude body1="rg2_gripper_base_link" body2="rg2_gripper_finger1_inner_knuckle_link"/>
        <exclude body1="rg2_gripper_base_link" body2="rg2_gripper_finger2_inner_knuckle_link"/>
        
